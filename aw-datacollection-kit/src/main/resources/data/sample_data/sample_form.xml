				<label ref="jr:itext('/sample_form/studied/math:label')"/>
				<value>math</value>
			</item>
			<item>
				<label ref="jr:itext('/sample_form/studied/pashto:label')"/>
				<value>pashto</value>
			</item>
			<item>
				<label ref="jr:itext('/sample_form/studied/dari:label')"/>
				<value>dari</value>
			</item>
			<item>
				<label ref="jr:itext('/sample_form/studied/english:label')"/>
				<value>english</value>
			</item>
			<item>
				<label ref="jr:itext('/sample_form/studied/arabic:label')"/>
				<value>arabic</value>
			</item>
		</select>
		<input ref="/sample_form/parcentage">
			<label ref="jr:itext('/sample_form/parcentage:label')"/>
		</input>
		<select1 ref="/sample_form/game">
			<label ref="jr:itext('/sample_form/game:label')"/>
			<item>
				<label ref="jr:itext('/sample_form/game/cricket:label')"/>
				<value>cricket</value>
			</item>
			<item>
				<label ref="jr:itext('/sample_form/game/football:label')"/>
				<value>football</value>
			</item>
			<item>
				<label ref="jr:itext('/sample_form/game/valleyball:label')"/>
				<value>valleyball</value>
			</item>
			<item>
				<label ref="jr:itext('/sample_form/game/other:label')"/>
				<value>other</value>
			</item>
		</select1>
		<input ref="/sample_form/other_game">
			<label ref="jr:itext('/sample_form/other_game:label')"/>
		</input>
		<upload mediatype="image/*" ref="/sample_form/photo">
			<label ref="jr:itext('/sample_form/photo:label')"/>
		</upload>
		<group ref="/sample_form/begin">
			<label ref="jr:itext('/sample_form/begin:label')"/>
			<repeat nodeset="/sample_form/begin">
				<upload mediatype="image/*" ref="/sample_form/begin/photo">
					<label ref="jr:itext('/sample_form/begin/photo:label')"/>
				</upload>
			</repeat>
		</group>
		<upload mediatype="audio/*" ref="/sample_form/audio_record">
			<label ref="jr:itext('/sample_form/audio_record:label')"/>
		</upload>
		<upload mediatype="video/*" ref="/sample_form/video_recored">
			<label ref="jr:itext('/sample_form/video_recored:label')"/>
		</upload>
		<input ref="/sample_form/geopoints">
			<label ref="jr:itext('/sample_form/geopoints:label')"/>
		</input>
		<upload mediatype="application/*" ref="/sample_form/some-file">
			<label ref="jr:itext('/sample_form/some-file:label')"/>
		</upload>
		<group appearance="field-list" ref="/sample_form/begin0">
			<input ref="/sample_form/begin0/date_time">
				<label ref="jr:itext('/sample_form/begin0/date_time:label')"/>
			</input>
			<input ref="/sample_form/begin0/starting">
				<label ref="jr:itext('/sample_form/begin0/starting:label')"/>
			</input>
			<input ref="/sample_form/begin0/times">
				<label ref="jr:itext('/sample_form/begin0/times:label')"/>
			</input>
		</group>
	</h:body>
