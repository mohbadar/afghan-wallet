			<instance>
				<sample_form id="Sample_Form">
					<name/>
					<age/>
					<gender/>
					<education/>
					<studied/>
					<parcentage/>
					<game/>
					<other_game/>
					<photo/>
					<begin jr:template="">
						<photo/>
					</begin>
					<audio_record/>
					<video_recored/>
					<geopoints/>
					<some-file/>
					<begin0>
						<date_time/>
						<starting/>
						<times/>
					</begin0>
					<meta>
						<instanceID/>
					</meta>
				</sample_form>
			</instance>
			<bind jr:requiredMsg="اسم خود را وارد نمایید" nodeset="/sample_form/name" required="true()" type="string"/>
			<bind jr:requiredMsg="عمر خود را وارد نمایید" nodeset="/sample_form/age" required="true()" type="int"/>
			<bind nodeset="/sample_form/gender" type="select1"/>
			<bind nodeset="/sample_form/education" type="select1"/>
			<bind nodeset="/sample_form/studied" type="select"/>
			<bind nodeset="/sample_form/parcentage" type="decimal"/>
			<bind nodeset="/sample_form/game" type="select1"/>
			<bind nodeset="/sample_form/other_game" relevant="selected( /sample_form/game , 'other')" type="string"/>
			<bind nodeset="/sample_form/photo" type="binary"/>
			<bind nodeset="/sample_form/begin/photo" type="binary"/>
			<bind nodeset="/sample_form/audio_record" type="binary"/>
			<bind nodeset="/sample_form/video_recored" type="binary"/>
			<bind nodeset="/sample_form/geopoints" type="geopoint"/>
			<bind nodeset="/sample_form/some-file" type="binary"/>
			<bind nodeset="/sample_form/begin0/date_time" type="dateTime"/>
			<bind nodeset="/sample_form/begin0/starting" type="date"/>
			<bind nodeset="/sample_form/begin0/times" type="time"/>
			<bind calculate="concat('uuid:', uuid())" nodeset="/sample_form/meta/instanceID" readonly="true()" type="string"/>
		</model>
	</h:head>
	<h:body>
		<input ref="/sample_form/name">
			<label ref="jr:itext('/sample_form/name:label')"/>
		</input>
		<input ref="/sample_form/age">
			<label ref="jr:itext('/sample_form/age:label')"/>
		</input>
		<select1 ref="/sample_form/gender">
			<label ref="jr:itext('/sample_form/gender:label')"/>
			<item>
				<label ref="jr:itext('/sample_form/gender/g_m:label')"/>
				<value>g_m</value>
			</item>
			<item>
				<label ref="jr:itext('/sample_form/gender/g_f:label')"/>
				<value>g_f</value>
			</item>
		</select1>
		<select1 appearance="minimal" ref="/sample_form/education">
			<label ref="jr:itext('/sample_form/education:label')"/>
			<item>
				<label ref="jr:itext('/sample_form/education/school:label')"/>
				<value>school</value>
			</item>
			<item>
				<label ref="jr:itext('/sample_form/education/university:label')"/>
				<value>university</value>
			</item>
		</select1>
		<select ref="/sample_form/studied">
			<label ref="jr:itext('/sample_form/studied:label')"/>
